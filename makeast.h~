#include <stdio.h>
#include <string.h>
#include <stdlib.h>

typedef enum {
  IDENT, //変数のノード型
  NUM, //整数のノード型
  ASSIGN, //代入文の　=　のノード型

  //PLUS, //加算演算子のノード型
  //MINUS, //減算演算子のノード型
  adsub, //加減算
  muldiv, //乗除算

  WHILE, // whileのノード型
  IF, // ifのノード型
  FOR, // forのノード型

  compari, //
  

} NType;

typedef struct node{
  Ntype type;
  int ivalue; // 整数の場合の値を入れる
  char* variable; // 変数の場合、変数名の文字列を入れる
  struct node *child;
  struct node *brother;
} Node;

